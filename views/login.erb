<h1>Log In</h1>


<form action= "/login" method="post">
  <div>
    <label for="user_name">User Name:</label>
    <input type="text" name="user_name"/>
  </div>
  <div>
    <label for="password">Password:</label>
    <input type="password" name="password"/>
  </div>
  <button type="submit">Log In</button>

</form>
